{
  "name": "new-sketchbook",
  "version": "1.0.0",
  "description": "new sketchbook website ",
  "main": "gulp.js",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.3",
    "critical": "^1.3.2",
    "del": "^3.0.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-cache": "^0.4.6",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-imagemin": "^3.3.0",
    "gulp-imageoptim": "^1.0.9",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-purgecss": "^0.21.0",
    "gulp-sass": "^3.1.0",
    "gulp-svg-sprite": "^1.4.0",
    "gulp-uglify": "^3.0.0",
    "gulp-useref": "^3.1.2",
    "gulp-util": "^3.0.8",
    "husky": "^1.2.1",
    "jshint-stylish": "^2.2.1",
    "lint-staged": "^8.1.0",
    "merge-stream": "^1.0.1",
    "prettier": "^1.15.3",
    "run-sequence": "^2.1.0",
    "stylelint": "^9.3.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-order": "^2.0.0"
  },
  "scripts": {
    "lint": "eslint --debug js/es6/index.js",
    "lint:write": "eslint --debug js/es6/index.js --fix",
    "prettier": "prettier --write js/es6/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.(css|scss)": [
        "stylelint --fix --config ./.stylelintrc-format",
        "git add"
      ],
      "*.js": [
        "npm run prettier",
        "npm run lint",
        "git add"
      ]
    },
    "ignore": [
      "**/sass/generic/_tiny-slider.scss",
      "**/js/**/*.js"
    ]
  },
  "author": "Anatoliy <mel198629@gmail.com> (http://artoko.org)",
  "license": "ISC",
  "dependencies": {
    "swiper": "^4.4.6"
  }
}
